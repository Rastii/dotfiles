Xcursor.theme: neutral

!-------------------------------------------------------------------------------
!
!
!      U R X V T
!
!
!URxvt*font:             -*-terminus-medium-r-*-*-14-140-*-*-*-*-*-*

! URxvt*font:             -*-terminus-medium-r-*-*-12-120-*-*-*-*-*-*
! URxvt*boldFont:         -*-terminus-medium-r-*-*-12-120-*-*-*-*-*-*
! URxvt*italicFont:       -*-terminus-medium-r-*-*-12-120-*-*-*-*-*-*
! URxvt*boldItalicFont:   -*-terminus-medium-r-*-*-12-120-*-*-*-*-*-*

URxvt*font:             xft:Source Code Pro:size=11
URxvt*boldFont:         xft:Source Code Pro:bold:size=11
!URxvt*font:              xft:Monaco:size=8

!URxvt*font:              xft:Ubuntu Mono:size=10
!URxvt*boldFont:          xft:Ubuntu Mono:bold:size=10

URxvt*saveLines:        32767
URxvt*scrollBar:        false
URxvt*scrollBar_right:  true
URxvt*loginShell:       true
URxvt*shading:          30
URxvt*termName:         rxvt-unicode
URxvt*secondaryScroll:  true
URxvt*perl-lib:         /usr/lib/urxvt/perl/
URxvt*perl-ext-common:  default,matcher,searchable-scrollback
URxvt*urlLauncher:      firefox
URxvt*matcher.button:   1
! URxvt*cutchars:         `"()'*<>[]{|}
URxvt*visualBell:       true
URxvt*keysym.C-u:       perl:matcher
URxvt*urgentOnBell:     true

! Solarized dark

URxvt*background:              #002b36
URxvt*foreground:              #839496
URxvt*fading:                  40
URxvt*fadeColor:               #002b36
URxvt*cursorColor:             #93a1a1
URxvt*pointerColorBackground:  #586e75
URxvt*pointerColorForeground:  #93a1a1

URxvt*color0:                  #073642
