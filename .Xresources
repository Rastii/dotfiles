Xft.dpi: 96
Xft.lcdfilter: lcddefault
Xft.antialias: true
Xft.autohint: true
Xft.hinting: true
Xft.hintstyle: hintslight
Xft.rgba: rgb
Xcursor.theme: neutral

!-------------------------------------------------------------------------------
!
!
!      U R X V T
!
!
URxvt*font:             -*-terminus-medium-r-*-*-14-140-*-*-*-*-*-*
URxvt*boldFont:         -*-terminus-medium-r-*-*-14-140-*-*-*-*-*-*
URxvt*italicFont:       -*-terminus-medium-r-*-*-14-140-*-*-*-*-*-*
URxvt*boldItalicFont:   -*-terminus-medium-r-*-*-14-140-*-*-*-*-*-*

! URxvt*font:             xft:DejaVu Sans Mono:size=10
! URxvt*boldFont:         xft:DejaVu Sans Mono:bold:size=10

! URxvt*font:              xft:Ubuntu Mono:size=10
! URxvt*boldFont:          xft:Ubuntu Mono:bold:size=10

URxvt*saveLines:        32767
URxvt*scrollBar:        false
URxvt*scrollBar_right:  true
URxvt*loginShell:       true
URxvt*shading:          30
URxvt*termName:         rxvt-unicode
URxvt*secondaryScroll:  true
URxvt*perl-lib:         /usr/lib64/urxvt/perl/
URxvt*perl-ext-common:  default,matcher,searchable-scrollback
URxvt*url-launcher:     xdg-open
URxvt*matcher.button:   1
URxvt*visualBell:       true
URxvt*keysym.C-u:       perl:matcher
URxvt*urgentOnBell:     true

!-------------------------------------------------------------------------------
!
!
!      X T E R M
!
!
XTerm*internalBorder:        0
XTerm*loginShell:         true
XTerm*scrollBar:         false
XTerm*cursorBlink:       false
!XTerm*geometry:         80x26
XTerm*saveLines:         65535
XTerm*dynamicColors:        on
XTerm*highlightSelection: true
! Catch URLs?
XTerm*charClass:     33:48,35-38:48,39:43,42-47:48,58-59:48,61:48,63-64:48,126:48
! Appearance
XTerm*utf8:          2
XTerm*eightBitInput: true
XTerm*faceName:      terminus:pixelsize=12


!TomorrowNight
#define t_background        #1d1f21
#define t_current_line      #282a2e
#define t_selection      	#373b41
#define t_foreground      	#c5c8c6
#define t_comment	     	#969896
#define t_red		     	#cc6666
#define t_orange	     	#de935f
#define t_yellow        	#f0c674
#define t_green	        	#b5bd68
#define t_aqua	        	#8abeb7
#define t_blue	        	#81a2be
#define t_purple        	#b294bb


URxvt*foreground:   t_foreground
URxvt*background:   t_background
URxvt*cursorColor:  #aeafad

! Black / Grey
URxvt*color0:       #000000
URxvt*color8:       #666666

! Red / Bright Red
URxvt*color1:       t_red
URxvt*color9:       #FF3334

! Green + Bright Green
URxvt*color2:       t_green
URxvt*color10:      #9ec400

! Yellow (Orange) + Bright Yellow (Yellow)
URxvt*color3:       t_orange
URxvt*color11:      t_yellow

! Blue + Bright Blue
URxvt*color4:       t_blue
URxvt*color12:      t_blue

! Magenta (Purple) + Bright Magenta
URxvt*color5:       t_purple
URxvt*color13:      #b777e0

! Cyan (Aqua) + Bright Cyan
URxvt*color6:       t_aqua
URxvt*color14:      #54ced6

! Light Grey (Selection) + White (Current Line)
URxvt*color7:       t_selection
URxvt*color15:      t_current_line

