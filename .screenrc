escape ^Qq
defscrollback 10000
deflogin on
altscreen on
autodetach on
msgwait 2
activity 'active: %n (%t) [%w:%s]'
bell 'bell: %n (%t) [%w:%s]^G'
vbell_msg '[[[ ding ]]]'
vbell off
startup_message off
defutf8 on
term screen-256color
termcapinfo rxvt-unicode 'Co#256:AB=\E[48;5;%dm:AF=\E[38;5;%dm'
termcapinfo rxvt-256color 'Co#256:AB=\E[48;5;%dm:AF=\E[38;5;%dm'

screen -t docker docker exec -it website-user bash
screen -t zsh zsh
screen -t zsh zsh
screen -t manager ssh root@manager
screen -t watch zsh -c "cd ~/lastline/website-user && ./watch.sh"
screen -t grunt-watch docker exec -it website-user grunt watch
# screen -t logs multitail -CS apache_error /var/log/apache2/error.log

bindkey -k k1 prev
bindkey -k k2 next

bindkey "^[Od" prev
bindkey "^[Oc" next

caption always "%{= kb}[ %{kG}$USER%{kW} :: %{kG}%H %{b}][ %=%{kW}%?%-Lw%?%{b}(%{W}%n*%f %t%?(%u)%?%{b})%{w}%?%+Lw%?%= %{b}][ %{W} %{G}%Y-%m-%d %{W}%c %{b} ]"
sorendition +b kG

select 1
